{{>header}}

<section>
      
      <h1> Login </h1>
      
      
      
      <form action="/users/login" method="POST">
          <label for="email">Email</label>
          <input
            type="email"
            id="email"
            name="email"
            class="formlogin"
            placeholder="Enter Email"
          />

        
          <label for="password">Password</label>
          <input
            type="password"
            id="password"
            name="password"
            class="formlogin"
            placeholder="Enter Password"
          />
        
        <button type="submit" >Login</button>
      </form>
      
      <a href="register.handlebars">Register</a>
   
</section>