{{>header}}
        
      <h1>Create Account</h1>
        
      <form action="/createaccount" name="createaccount" method="POST">
        <fieldset>
          <label for="firstname">First name</label>
          <input
            type="fname"
            id="fname"
            name="fname"
            placeholder="Enter Name"
            value=""
          />
        </fieldset>

        <fieldset>
          <label for="lastname">Last name</label>
          <input
            type="lname"
            id="lname"
            name="lname"
            placeholder="Enter Name"
            value=""
          />
        </fieldset>


        <fieldset>
          <label for="email">Email</label>
          <input
            type="email"
            id="email"
            name="email"
            placeholder="Enter Email"
            value=""
          />
        </fieldset>
        <fieldset>
          <label for="password">Password</label>
          <input
            type="password"
            id="password"
            name="password"
            placeholder="Create Password"
            value=""
          />
        </fieldset>
        <fieldset>
          <label for="password2">Confirm Password</label>
          <input
            type="password"
            id="password2"
            name="password2"
            placeholder="Confirm Password"
            value=""
          />
        </fieldset>
        
        <button type="submit" form="createaccount">
          Register
        </button>
        
      </form>
        
      <a href="login">Back</a>  
